{
  "name": "pvsaintpe/yii2-pager",
  "description": "Package",
  "keywords": [
    "yii2",
    "gridview"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Veselov",
      "email": "pvsaintpe@icloud.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "2.*",
    "kartik-v/yii2-krajee-base": "*",
    "kartik-v/yii2-grid": "*",
    "kartik-v/yii2-widgets": "*",
    "pvsaintpe/yii2-helpers": "*",
    "kartik-v/yii2-widget-touchspin": "*"
  },
  "autoload": {
    "psr-4": {
      "pvsaintpe\\pager\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
